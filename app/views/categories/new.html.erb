<%= provide(:title, 'Categories') %>

<h1>Create a new Category</h1>

<div class="row">
	<div class="span6 offset3">
		<%= form_for @category, url: user_categories_path do |f| %>

			
			<%= render 'cat_fields', f: f %>
					
			<%= f.submit "Save", class: "btn btn-large btn-primary" %>
		<% end %>
	</div>

</div>