<fieldset class="form-inline">
  <%= f.label :entrytime, "Timestamp" %>  			  	
  <%= f.datetime_local_field :entrytime, class: "span3 control-label", prompt: Time.now.strftime("%Y-%m-%dT%R")  %>

	<%= f.label :quantity %>
	<%= f.text_field :quantity, class: "span1" %>

	<%= f.label :price %>
	<%= f.text_field :price, class: "input-small"%>	
	<%= f.hidden_field :_destroy %>
	<%= link_to "remove", '#', class: "remove_fields" %>
</fieldset>